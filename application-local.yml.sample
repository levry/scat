spring:
  datasource:
    driver-class-name: org.h2.Driver
    url: jdbc:h2:file:./.data/scat;IGNORECASE=true
    username: admin
    password: admin
  jpa:
    hibernate:
      ddl-auto: none
    show-sql: true
    properties:
      hibernate.dialect: org.hibernate.dialect.H2Dialect

logging.level.web: debug
